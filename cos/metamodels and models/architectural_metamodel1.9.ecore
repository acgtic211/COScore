    <eStructuralFeatures xsi:type="ecore:EReference" name="naryRelationship" eType="#//Nary"
        eOpposite="#//Nary/bRelationship"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
        eType="#//AbstractComponent" eOpposite="#//AbstractComponent/bSource"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
        eType="#//AbstractComponent" eOpposite="#//AbstractComponent/bTarget"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dependency" lowerBound="1"
        upperBound="-1" eType="#//AbstractDependency" containment="true" eOpposite="#//AbstractDependency/bRelationship"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Nary" eSuperTypes="#//Relationship">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" lowerBound="1" eType="#//NaryType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="bRelationship" lowerBound="2"
        upperBound="-1" eType="#//Binary" eOpposite="#//Binary/naryRelationship"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AbstractArchitecturalModel">
    <eStructuralFeatures xsi:type="ecore:EReference" name="abstractComponent" upperBound="-1"
        eType="#//AbstractComponent" containment="true" eOpposite="#//AbstractComponent/aam"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="relationship" upperBound="-1"
