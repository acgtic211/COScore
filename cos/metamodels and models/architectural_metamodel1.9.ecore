    <eStructuralFeatures xsi:type="ecore:EAttribute" name="aamID" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ConcreteArchitecturalModel">
    <eStructuralFeatures xsi:type="ecore:EReference" name="concreteComponent" upperBound="-1"
        eType="#//ConcreteComponent" containment="true" eOpposite="#//ConcreteComponent/cam"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="aam" lowerBound="1" eType="#//AbstractArchitecturalModel"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="camID" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="relationship" upperBound="-1"
        eType="#//Relationship" containment="true" eOpposite="#//Relationship/cam"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="aamID" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ConcreteComponent" eSuperTypes="#//AbstractComponent">
    <eStructuralFeatures xsi:type="ecore:EReference" name="cam" lowerBound="1" eType="#//ConcreteArchitecturalModel"
        eOpposite="#//ConcreteArchitecturalModel/concreteComponent"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="runtimeProperty" upperBound="-1"
        eType="#//RuntimeProperty" containment="true" eOpposite="#//RuntimeProperty/cc"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="port" lowerBound="1" upperBound="-1"
        eType="#//Port" containment="true" eOpposite="#//Port/cc"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="componentInstance" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        defaultValueLiteral=""/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ComponentType">
    <eLiterals name="normal"/>
    <eLiterals name="container" value="1"/>
    <eLiterals name="functional" value="2"/>
    <eLiterals name="userInteraction" value="3"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InputPort" eSuperTypes="#//Port">
    <eStructuralFeatures xsi:type="ecore:EReference" name="cTarget" upperBound="-1"
        eType="#//Connector" eOpposite="#//Connector/target"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OutputPort" eSuperTypes="#//Port">
    <eStructuralFeatures xsi:type="ecore:EReference" name="cSource" upperBound="-1"
        eType="#//Connector" eOpposite="#//Connector/source"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RuntimeProperty">
    <eStructuralFeatures xsi:type="ecore:EReference" name="cc" lowerBound="1" eType="#//ConcreteComponent"
        eOpposite="#//ConcreteComponent/runtimeProperty"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="propertyID" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="propertyValue" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Interface" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="component" eType="#//AbstractComponent"
        eOpposite="#//AbstractComponent/interface"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="interfaceID" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Provided" eSuperTypes="#//Interface">
    <eStructuralFeatures xsi:type="ecore:EReference" name="dTarget" upperBound="-1"
        eType="#//AbstractDependency" eOpposite="#//AbstractDependency/target"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Required" eSuperTypes="#//Interface">
    <eStructuralFeatures xsi:type="ecore:EReference" name="dSource" upperBound="-1"
        eType="#//AbstractDependency" eOpposite="#//AbstractDependency/source"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AbstractDependency">
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
        eType="#//Required" eOpposite="#//Required/dSource"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="bRelationship" lowerBound="1"
        eType="#//Binary" eOpposite="#//Binary/dependency"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
        eType="#//Provided" eOpposite="#//Provided/dTarget"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="dependencyID" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ConcreteDependency" eSuperTypes="#//AbstractDependency">
    <eStructuralFeatures xsi:type="ecore:EReference" name="connector" lowerBound="1"
        upperBound="-1" eType="#//Connector" containment="true" eOpposite="#//Connector/dependency"/>
  </eClassifiers>
</ecore:EPackage>
