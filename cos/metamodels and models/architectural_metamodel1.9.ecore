<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="architectural_metamodel" nsURI="http://architectural_metamodel/1.9"
    nsPrefix="architectural_metamodel">
  <eClassifiers xsi:type="ecore:EClass" name="AbstractComponent">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="componentName" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="interface" lowerBound="1"
        upperBound="-1" eType="#//Interface" containment="true" eOpposite="#//Interface/component"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="componentAlias" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="componentType" lowerBound="1"
        eType="#//ComponentType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="aam" eType="#//AbstractArchitecturalModel"
        eOpposite="#//AbstractArchitecturalModel/abstractComponent"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="bSource" upperBound="-1"
        eType="#//Binary" eOpposite="#//Binary/source"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="bTarget" upperBound="-1"
        eType="#//Binary" eOpposite="#//Binary/target"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Port" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="portID" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="cc" lowerBound="1" eType="#//ConcreteComponent"
        eOpposite="#//ConcreteComponent/port"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Connector">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="connectorID" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
        eType="#//InputPort" eOpposite="#//InputPort/cTarget"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
        eType="#//OutputPort" eOpposite="#//OutputPort/cSource"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dependency" lowerBound="1"
        eType="#//ConcreteDependency" eOpposite="#//ConcreteDependency/connector"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Relationship" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="relationshipID" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="aam" eType="#//AbstractArchitecturalModel"
        eOpposite="#//AbstractArchitecturalModel/relationship"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="cam" eType="#//ConcreteArchitecturalModel"
        eOpposite="#//ConcreteArchitecturalModel/relationship"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="BinaryType">
    <eLiterals name="association"/>
    <eLiterals name="composition" value="2"/>
    <eLiterals name="dependence" value="4"/>
    <eLiterals name="inheritance" value="5"/>
    <eLiterals name="producerconsumer" value="9"/>
    <eLiterals name="___" value="10"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="NaryType">
    <eLiterals name="sequence" value="1"/>
    <eLiterals name="hierarchy" value="2"/>
    <eLiterals name="trading" value="3"/>
    <eLiterals name="observer" value="4"/>
    <eLiterals name="controller" value="5"/>
    <eLiterals name="___" value="6"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Binary" eSuperTypes="#//Relationship">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" lowerBound="1" eType="#//BinaryType"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="isBidirectional" lowerBound="1"
